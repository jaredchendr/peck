<div class="container">
	<div class="indexer">
<%= form_tag indexer_path,:method => :get do %>
  <p>
  <%= text_field_tag :search,params[:search] ,:id => "search_first" %>
  <%= submit_tag "search",:name => nil ,:id => "search_first_submit"%>
<% end %>
<ul>
	<label>找到约</label><%= @totals %><label>个结果</label>
  	<% @results.each do |result| %>
  	<li ><%= link_to result.name, result%>:<%= result.defi %>
  		<strong>症状</strong>:<%= result.symp %>
    	</li>
    	<% end %>
</ul>
</div>
</div>
